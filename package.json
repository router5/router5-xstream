{
  "name": "router5-xstream",
  "version": "4.0.0",
  "description": "xstream plugin for router5",
  "main": "dist/commonjs/index.js",
  "jsnext:main": "dist/es/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "build:es": "BABEL_ENV=es babel modules --out-dir dist/es",
    "build": "rimraf dist && npm run build:cjs && npm run build:es",
    "lint": "eslint modules",
    "test": "mocha --compilers js:babel-core/register test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/router5/router5-xstream.git"
  },
  "keywords": [
    "router",
    "xstream",
    "reactive",
    "stream",
    "router5"
  ],
  "author": "Thomas Roch <thomas.c.roch@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/router/router5-xstream/issues"
  },
  "homepage": "http://router5.github.io",
  "dependencies": {
    "router5.transition-path": "^3.0.0",
    "xstream": "^4.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "chai": "^3.5.0",
    "eslint": "^2.11.1",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "router5": "^3.0.0"
  }
}
